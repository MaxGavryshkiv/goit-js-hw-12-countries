{{#each [] }}
<div class="country-text-box">
<h1 class="country-name">{{name}}</h1>
<p class="country-capital">Capital: {{capital}}</p>
<p class="country-population">Population: {{population}}</p>
<p class="country-languages-header">Languages:</p>
<ul class="country-languages">
{{#each languages}}
<li class="country-language">{{this.name}}</li>
{{/each}}
</ul>
</div>
<img class="country-flag" src="{{flag}}" alt="{{name}}" />
{{/each}}